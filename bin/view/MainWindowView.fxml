<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainWindowController">
   <children>
      <Label fx:id="label" alignment="CENTER" layoutY="122.0" text="Label" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" layoutX="150.0" layoutY="187.0" spacing="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <TextField fx:id="field" />
            <Button mnemonicParsing="false" onAction="#handleButton" text="change Text" />
            <Button mnemonicParsing="false" onAction="#showAlert1" text="Alert1" />
            <Button mnemonicParsing="false" onAction="#showAlert2" text="Alert2" />
            <Button mnemonicParsing="false" onAction="#showAlert3" text="Alert3" />
            <Button mnemonicParsing="false" onAction="#showAlert4" text="Alert4" />
            <Button mnemonicParsing="false" onAction="#showAlert5" text="Alert5" />
         </children>
      </HBox>
   </children>
</AnchorPane>
